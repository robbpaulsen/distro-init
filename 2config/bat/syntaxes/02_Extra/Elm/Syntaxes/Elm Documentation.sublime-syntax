  main:
    - match: '\x{FEFF}'
      comment: Code Block
      push:
        - meta_scope: markup.raw.block.elm-documentation
        - meta_content_scope: markup.raw.block.elm-documentation
        - match: '\x{FEFF}'
          pop: true
        - include: scope:source.elm
